<?xml version="1.0"?>
<!DOCTYPE ROOT SYSTEM "qud2.dtd">
<ROOT> 
    <QUD string = "Was ist mit dem neuen Audi SQ5?">
        <FEEDER>
            <RES><SEGMENT> Zunächst </SEGMENT></RES>
            <F><SEGMENT> ein Blick in </SEGMENT></F> 
            <CON><SEGMENT> die technischen Daten: </SEGMENT></CON>
        </FEEDER>
        <QUD string = "Was ist mit den technischen Daten zum SQ5?">
            <!-- DT: (Technische Daten) Was der Hersteller verspricht -->
            <QUD string = "Was ist mit der Leistung des Motors?">
                <F><SEGMENT id="1.1"> Der Dreiliter-Sechszylinder </SEGMENT></F>
                <F><SEGMENT id="1.2"> leistet 255 kW/347 PS. </SEGMENT></F>
            </QUD>
            <QUD string = "Was ist mit dem Standardspurt des SQ5? / Beschleunigung?">
                <CON><SEGMENT> Der Standardspurt </SEGMENT></CON>
                <F><SEGMENT> gelingt in 5,1 Sekunden,  </SEGMENT></F>
            </QUD>
            <QUD string = "Was ist mit der Höchstgeschwindigkeit des SQ5?">
                <CON><SEGMENT> die Höchstgeschwindigkeit </SEGMENT></CON>
                <F><SEGMENT> ist bei 250 km/h abgeregelt, </SEGMENT></F>
            </QUD>
            <QUD string = "Was ist mit der Kraftübertragung (Getriebe) des SQ5?"> 
                <CON><SEGMENT> die Kraftübertragung </SEGMENT></CON>
                <F><SEGMENT> erfolgt über eine Achtgang-Automatik. </SEGMENT></F>
            </QUD>
            <QUD string = "Was gehört beim Getriebe ab Werk dazu?">
                <NAI><SEGMENT> Der bei Audi „quattro“ genannte </SEGMENT></NAI>
                <F><SEGMENT id="1.5.1"> permanente Allradantrieb </SEGMENT></F>
                <CON><SEGMENT id="1.5.3"> gehört </SEGMENT></CON>
                <RES> samt </RES><!-- und -->
                <F><SEGMENT id="1.5.2"> selbstsperrendem Mittendifferenzial </SEGMENT></F>
                <CON><SEGEMENT id="1.5.3"> (gehört) ab Werk dazu. </SEGEMENT></CON>
            </QUD>
            <QUD string = "Was ist mit dem Durschnittsverbrauch laut Hersteller?"> 
                <!-- DT: Umwelt/Emissionen-->
                <QUD string = "Was ist der Durchschnittsverbrauch des SQ5?">
                    <CON><SEGMENT> Den Durchschnittsverbrauch </SEGMENT></CON>
                    <F><SEGMENT id="1.6.1.1"> gibt Audi mit Werten zwischen 6,6 </SEGMENT></F> 
                    <RES><SEGMENT> und </SEGMENT></RES>
                    <F><SEGMENT id="1.6.1.1"> 6,8 Litern an, </SEGMENT></F> 
                    <QUD string = "Wie ist der CO2-Ausstoß des SQ5?">
                        <CON><SEGMENT id="1.6.2.1"> das </SEGMENT></CON>
                        <F><SEGMENT id="1.6.2.2"> entspricht </SEGMENT></F>
                        <CON><SEGMENT id="1.6.2.1"> einem CO2-Ausstoß </SEGMENT></CON>
                        <F><SEGMENT id="1.6.2.2"> von 172 bis 177 g/km. </SEGMENT></F>
                        <QUD string = "Wie der CO2-Ausstoß des SQ5 im Vergleich zum vorherigen Benziner-Angebot?">
                            <!-- dass der Vorgänger des SQ5 ein Benziner war, wurde in Einleitung erklärt-->
                            <CON><SEGMENT> Im Vergleich zum vorigen Benziner-Angebot </SEGMENT></CON> 
                            <F><SEGMENT> ist das eine Reduzierung von rund 20 Gramm. </SEGMENT></F>
                        </QUD>
                        <QUD string = "Was ist mit den vergleichsweise guten Normwerten in Bezug auf den CO2-Ausstoß des SQ5?">
                            <CON>
                                <SEGMENT> Die </SEGMENT>
                                <NAI><SEGMENT> vergleichsweise guten </SEGMENT></NAI><!-- kein echter Vergleich, sondern nur eine Phrase -->
                                <SEGMENT> Normwerte </SEGMENT>
                            </CON>
                            <F><SEGMENT id="1.6.3.2.1"> werden </SEGMENT></F>
                            <RES><SEGMENT> unter anderem </SEGMENT></RES><!-- unter anderem Fahrweise -->
                            <F><SEGMENT id="1.6.3.2.1"> durch den Einsatz eines 48-Volt-Systems möglich, </SEGMENT></F>
                            <QUD string = "Was ist eine weitere Funktion des 48-Volt-Systems?">
                                <CON><SEGMENT> das </SEGMENT></CON>
                                <RES><SEGMENT> auch </SEGMENT></RES>
                                <F><SEGMENT> für den Betrieb eines Riemenstartergenerators genutzt wird. </SEGMENT></F>
                                <QUD string = "Was tut der Riemenstartergenerator?">
                                    <CON><SEGMENT> Der </SEGMENT></CON>
                                    <F><SEGMENT id="1.6.3.2.1.1.1"> ermöglicht ein effizientes Start-Stopp-System mit Segelfunktion </SEGMENT></F>
                                    <RES><SEGMENT> sowie </SEGMENT></RES><!-- und -->
                                    <F><SEGMENT id="1.6.3.2.1.1.2"> (ermöglicht) eine gute Rekuperation. </SEGMENT></F>
                                </QUD>
                            </QUD>
                        </QUD>
                    </QUD>
                </QUD>
            </QUD>
            <QUD string = "Was sprich noch für den Diesel?">
                <FEEDER>
                    <SEGMENT id="2.1.1.1"> Als weiterer Grund für den Diesel kann auch das  </SEGMENT>
                    <NAI><SEGMENT> üppige </SEGMENT></NAI>
                    <SEGMENT id="2.1.1.2"> Drehmoment gesehen werden. </SEGMENT>
                </FEEDER>
                <QUD string = "Was ist mit dem Drehmoment?"> 
                    <CON><SEGMENT> Es </SEGMENT></CON>
                    <F><SEGMENT id="2.1.1.1.1"> steht zwischen 2.500 </SEGMENT></F>
                    <RES><SEGMENT> und </SEGMENT></RES>
                    <F><SEGMENT id="2.1.1.1.2"> 3.100 Umdrehungen bereit. </SEGMENT></F>
                </QUD>
            </QUD>
            <QUD string = "Was ist mit dem Turboloch?">
                <F><SEGMENT id="2.1.2.1"> Damit kein </SEGMENT></F> 
                <CON><SEGMENT> Turboloch </SEGMENT></CON>
                <F><SEGMENT id="2.1.2.1"> entsteht, </SEGMENT></F>
                <QUD string = "Was tut der Motor damit kein Turboloch entsteht?">
                    <F><SEGMENT id="2.1.2.1.1"> verfügt </SEGMENT></F> 
                    <CON><SEGMENT> das V6-Aggregat </SEGMENT></CON>
                    <F>
                        <SEGMENT id="2.1.2.1.1"> neben dem </SEGMENT>
                        <NAI><SEGMENT> konventionellen </SEGMENT></NAI>
                        <SEGMENT id="2.1.2.1.1"> Abgasturbolader </SEGMENT>
                    </F> 
                    <F><SEGMENT id="2.1.2.1.2"> über einen elektrischen Verdichter, </SEGMENT></F>
                    <QUD string = "Was tut der elektrischen Verdichter zur Vermeidung des Turbolochs?">
                        <CON><SEGMENT> der </SEGMENT></CON>
                        <F><SEGMENT id="2.1.2.1.1.1"> einspringt, </SEGMENT></F>
                        <RES><SEGMENT> wenn </SEGMENT></RES>
                        <F><SEGMENT id="2.1.2.1.1.1"> der abgasbetriebene Lader noch nicht über genug Druck verfügt. </SEGMENT></F>
                    </QUD> 
                </QUD>
            </QUD>
        </QUD>
        <QUD string = "Was ist mit dem Fahrgefühl beim SQ5?">
            <!-- DT: (Fahrgefühl) Erfahrungswerte aus der Testfahrt -->
            <FEEDER>
                <SEGMENT> Soweit die Theorie. </SEGMENT>
            </FEEDER> <!-- feeder?-->
            <QUD string = "Wie das Fahrgefühl?">
                <QUD string = "Was ist mit Effekt der Technik auf das Fahrgefühl?">
                    <QUD string = "Was ist mit der Auswirkung der Technik auf den Fahrer?">
                        <CON><SEGMENT> Der Fahrer </SEGMENT></CON>
                        <F><SEGMENT id="2.2.1.1"> merkt </SEGMENT></F>
                        <CON>
                            <SEGMENT> von der </SEGMENT>
                            <SEGMENT> ausgeklügelten </SEGMENT><!-- Wertung des Autors + Komplexität, die für Argument wichtig ist -->
                            <SEGMENT> Technik </SEGMENT>
                        </CON>
                        <F><SEGMENT id="2.2.1.1"> wenig </SEGMENT></F>
                        <RES><SEGMENT> und </SEGMENT></RES>
                        <RES><SEGMENT> doch </SEGMENT></RES>
                        <NAI><SEGMENT> irgendwie </SEGMENT></NAI><!-- bezogen auf "den Effekt spürt der Fahrer dagegen ganz genau." -->
                        <RES><SEGMENT> auch wieder </SEGMENT></RES>
                        <F><SEGMENT id="2.2.1.1">  viel. </SEGMENT></F>
                        <QUD string = "Wovon merkt der Fahrer in Bezug auf die Technik wenig?"><!-- Achtung: Fokus/Background wechseln -->
                            <QUD String = "Was passiert im Hintergrund?">
                                <F><SEGMENT> Dass unter der Haube ein technisches Abstimmungsballett stattfindet, </SEGMENT></F> 
                                <!-- das ist CON, weil die Technik schon im vorherigen Absatz beschrieben wurde (Abgasturbolader, elektrischer Verdichter etc)-->
                                <CON>
                                    <SEGMENT id="2.2.1.1.1"> passiert </SEGMENT>
                                    <NAI><SEGMENT> dezent </SEGMENT></NAI>
                                    <SEGMENT id="2.2.1.1.1"> im Hintergrund. </SEGMENT>
                                </CON>
                            </QUD>
                        </QUD>
                        <QUD string = "Wovon merkt der Fahrer in Bezug auf die Technik viel?">
                            <F><SEGMENT> Den Effekt </SEGMENT></F>
                            <CON><SEGMENT> spürt der Fahrer dagegen ganz genau. </SEGMENT></CON>
                            <QUD string = "Was ist der Effekt der Technik?">
                                <QUD string = "Was ist mit dem Drehmoment in Bezug auf das Fahrgefühl?">
                                    <CON><SEGMENT> Drehmoment </SEGMENT></CON>
                                    <F><SEGMENT> steht bereits mit der ersten leichten Gasfußbewegung bereit. </SEGMENT></F> <!-- wegen dem Verdichter -->
                                </QUD>
                                <QUD string = "Was ist mit Anstrengung (des Motors) und Turboloch in Bezug auf das Fahrgefühl?">
                                    <CON><SEGMENT id="2.2.1.2.1.2.1"> Von Anstrengung, </SEGMENT></CON>
                                    <CON><SEGMENT id="2.2.1.2.1.2.2"> Turboloch </SEGMENT></CON>
                                    <RES><SEGMENT> oder </SEGMENT></RES>
                                    <RES><SEGMENT> auch </SEGMENT></RES>
                                    <CON><SEGMENT id="2.2.1.2.1.2.3"> nur Löchlein </SEGMENT></CON>
                                    <F><SEGMENT> ist nichts zu spüren. </SEGMENT></F>
                                </QUD>
                                <QUD string = "Worauf muss der Fahrer im Stadtverkehr oder auf limitierten Strecken aufpassen?">
                                    <CON><SEGMENT id="2.2.1.2.1.3.1"> Man muss </SEGMENT></CON>
                                    <RES><SEGMENT> eher </SEGMENT></RES><!-- impliziter Kontrast zum Turboloch -->
                                    <CON><SEGMENT  id="2.2.1.2.1.3.1"> aufpassen, </SEGMENT></CON>
                                    <F><SEGMENT  id="2.2.1.2.1.3.2"> die schon bei leichter Berührung des Gaspedals stürmisch los eilenden Pferde </SEGMENT></F>
                                    <CON><SEGMENT  id="2.2.1.2.1.3.1"> im Stadtverkehr </SEGMENT></CON>
                                    <RES><SEGMENT> oder </SEGMENT></RES>
                                    <CON><SEGMENT  id="2.2.1.2.1.3.1"> auf limitierten Strecken </SEGMENT></CON><!-- limitierte Geschwindigkeit -->
                                    <F><SEGMENT id="2.2.1.2.1.3.2"> im Zaum </SEGMENT></F>
                                    <RES><SEGMENT> und </SEGMENT></RES><!-- und damit/deshalb -->
                                    <F><SEGMENT id="2.2.1.2.1.3.2"> innerhalb der Straßenverkehrsvorschriften zu halten. </SEGMENT></F>
                                    <QUD string = "Was ist ganz klar?">
                                        <CON><SEGMENT> Ganz klar: </SEGMENT></CON>
                                        <QUD string = "Was wollen die Pferde tun?">
                                            <CON><SEGMENT> die </SEGMENT></CON>
                                            <F><SEGMENT id="2.2.1.2.1.3.1.1"> wollen nicht spielen, </SEGMENT></F>
                                            <CON><SEGMENT> die </SEGMENT></CON>
                                            <F><SEGMENT id="2.2.1.2.1.3.1.1"> wollen wild galoppieren. </SEGMENT></F>
                                        </QUD>
                                    </QUD>
                                </QUD>
                            </QUD>
                        </QUD>
                    </QUD>
                </QUD>
                <QUD string = "Was ist mit dem Fahrgefühl auf der Testfahrt?">
                    <!-- DT: Fahrgefühl auf Testfahrt-->
                    <QUD string = "Welche techischen Aspekte wurden bei der Testfahrt getestet?">
                        <CON>
                            <NAI><Segment> Zum Glück </Segment></NAI>
                            <Segment> gab es bei ersten Testfahrten die Möglichkeit, </Segment>
                        </CON>
                        <F><Segment id = "2.3.1.1">  das spontane Ansprechverhalten des Triebwerks (zu testen) </Segment></F>
                        <F><Segment id = "2.3.1.2"> das bärenstarke Drehmoment (zu testen) </Segment></F>
                        <RES><Segment> sowie </Segment></RES><!-- und -->
                        <F><Segment id = "2.3.1.3"> die gelungene Fahrwerksabstimmung (zu testen) </Segment></F>
                        <QUD String = "Wo wurden diese technischen Aspekte getestet?">
                            <F><Segment id = "1.3.2.2.1"> im kurvigen (Geläuf) </Segment></F>
                            <RES><Segment> und </Segment></RES>
                            <F><Segment id = "1.3.2.2.2"> wenig befahrenen Geläuf zu testen.</Segment></F>
                        </QUD>
                    </QUD>
                    <QUD string = "Wie war die Kurvenlage bei der Testfahrt?">
                        <QUD string = "Was erleichtert das Kurvendurcheilen?">
                            <F><SEGMENT id="2.3.2.1"> Unproblematische </SEGMENT></F>
                            <RES><SEGMENT> und </SEGMENT></RES>
                            <F><SEGMENT id="2.3.2.1"> schnelle Lastwechsel, </SEGMENT></F>
                            <F><SEGMENT id="2.3.2.2"> Spurtreue </SEGMENT></F>
                            <RES><SEGMENT> und </SEGMENT></RES>
                            <F><SEGMENT id="2.3.2.3"> eine flott agierende Automatik </SEGMENT></F>
                            <CON><Segment> machen das Kurvendurcheilen einfach, </Segment></CON>
                        </QUD>
                        <QUD string = "Was fällt bei der Innenausstattung in Kurvenfahrten auf?">
                            <QUD string = "Was ist an den Sitzen erfreulich in Kurven?"> 
                                <RES><SEGMENT> auch wenn </SEGMENT></RES>
                                <CON><SEGMENT id="2.3.3.1.1"> man sich </SEGMENT></CON>
                                <RES><SEGMENT> hier </SEGMENT></RES>
                                <CON>
                                    <NAI><SEGMENT> besonders </SEGMENT></NAI>
                                    <SEGMENT id="2.3.3.1.1"> über die </SEGMENT>
                                    <NAI><SEGMENT> guten </SEGMENT></NAI>
                                    <SEGMENT id="2.3.3.1.1"> Sportsitze </SEGMENT>
                                </CON>
                                <F><SEGMENT id="2.3.3.1.1"> mit viel Seitenhalt </SEGMENT></F>
                                <CON><SEGMENT id="2.3.3.1.1"> freut. </SEGMENT></CON>
                            </QUD>
                        </QUD>
                        <QUD string = "Nach welcher Art von Auto fühlt sich der SQ5 in Kurven an?">
                            <QUD String = "Was bemerkt man kaum (an dieser Stelle = in Kurven)?">
                                <RES><Segment> Dass </Segment></RES>
                                <F><Segment id = "1.3.3.5.1"> man in einem 2-Tonnen-SUV (sitzt) </Segment></F>
                                <RES><Segment> und </Segment></RES>
                                <F><Segment id = "1.3.3.5.2"> nicht in einem flachen Sportwagen sitzt, </Segment></F>
                                <CON><Segment> bemerkt man an dieser Stelle kaum. </Segment></CON>
                            </QUD>
                        </QUD>
                    </QUD>
                    
                    <QUD string = "Was ist mit Wankbewegungen (des SQ5 auf der Testfahrt in Kurven)?">
                        <RES><SEGMENT> Auch </SEGMENT></RES>
                        <CON><SEGMENT> Wankbewegungen </SEGMENT></CON>
                        <F><SEGMENT> sind nicht auszumachen. </SEGMENT></F>
                    </QUD>
                </QUD>
                <QUD string = "Welche Fahrmodi gibt es?"><!-- Sport- und Komfortmodus -->
                <!-- DT: Modi -->
                    <QUD string = "Gibt es außer dem Sportmodus noch andere Modi? / Kann man den Fahrmodus wechseln?">
                        <QUD string = "Warum sollte man den Fahrmodus wechseln wollen?">
                            <F><SEGMENT> Registriert man eine leicht grünliche oder blässliche Verfärbung im Gesicht des Beifahrers </SEGMENT></F>
                        </QUD>
                        <F>
                            <SEGMENT id="2.4.1.1.1"> hilft es </SEGMENT>
                            <NAI><SEGMENT> vermutlich </SEGMENT></NAI>
                        </F>
                        <RES><SEGMENT> schon, </SEGMENT></RES>
                        <F><SEGMENT id="2.4.1.1.1"> vom hart abgestimmten Sportmodus in die Komfortstellung zu wechseln. </SEGMENT></F>
                    </QUD>
                    
                    <QUD string = "Was ist mit Unterschieden zwischen den Modi/Abstimmungen?">
                        <CON><SEGMENT> Die unterschiedlichen Abstimmungen </SEGMENT></CON> <!-- CON weil Abstimmung = Modus-->
                        <F><SEGMENT id="2.4.2.1"> sind deutlich zu bemerken - </SEGMENT></F>
                        <RES><SEGMENT> und </SEGMENT></RES>
                        <RES><SEGMENT> auch </SEGMENT></RES>
                        <F><SEGMENT id="2.4.2.2"> hörbar. </SEGMENT></F>
                        <QUD string = "Wie klingt der SQ5 im Sportmodus?">
                            <F><SEGMENT id="2.4.2.1.1"> Dringt in den Innenraum </SEGMENT></F>
                            <CON><SEGMENT> während der sportlichen Straffung </SEGMENT></CON>
                            <F>
                                <NAI><SEGMENT id="2.4.2.1.1"> ein satter, </SEGMENT></NAI>
                                <SEGMENT id="2.4.2.1.1"> künstlich generierter </SEGMENT>
                            </F>
                            <RES><SEGMENT> und </SEGMENT></RES>
                            <F>
                                <NAI><SEGMENT id="2.4.2.1.1"> V8-ähnlicher </SEGMENT></NAI>
                                <SEGMENT id="2.4.2.1.1"> Dröhnsound, </SEGMENT>
                            </F>
                        </QUD>
                        <QUD string = "Wie klingt der SQ5 im Komfortmodus?">
                            <F><SEGMENT id="2.4.2.2.1"> werden die Ohren der Insassen </SEGMENT></F>
                            <CON><SEGMENT> im Komfortmodus </SEGMENT></CON>
                            <F><SEGMENT id="2.4.2.2.1"> geschont. </SEGMENT></F>
                        </QUD>
                    </QUD>
                </QUD>
            </QUD>
        </QUD>


        <QUD string = "Was ist mit dem Kraftstoffverbrauch auf der Testfahrt/tatsächlicher Verbrauch?">
            <QUD string = "Auf was sollte man beim Blick auf die Verbrauchswerte (den Bordcomputer) gefasst sein?">
                <QUD string = "Wie sollte man sich vorbereiten (auf die Blick auf den Bordcomputer)?">
                    <QUD string = "Wie hängen Wahl des Modus und Kraftstoffverbrauch zusammen?"> 
                        <F><SEGMENT id="3.1.1"> Gleichgültig in welchem </SEGMENT></F>
                        <CON><SEGMENT> Modus </SEGMENT></CON>
                        <F><SEGMENT id="3.1.1"> man sich befindet, </SEGMENT></F>
                    </QUD>
                    <F><SEGMENT id="3.2.1"> ein wenig abgehärtet sollte man </SEGMENT></F>
                    <RES><SEGMENT> indes </SEGMENT></RES><!-- hingegen, dennoch / dabei -->
                    <RES><SEGMENT> schon </SEGMENT></RES>
                    <F><SEGMENT id="3.2.1"> sein, </SEGMENT></F>
                    <RES><SEGMENT> wenn </SEGMENT></RES>
                    <CON><SEGMENT> man auf den Bordcomputer blickt. </SEGMENT></CON>
                </QUD>
                <QUD string = "Was ist mit dem Normwert zum Kraftstoffverbrauch auf der Testfahrt bei langsamer Fahrt?">
                    <CON><SEGMENT> Der Normwert </SEGMENT></CON>
                    <F><SEGMENT> wird selbst bei gemütlichem Dahinschlendern deutlich verfehlt. </SEGMENT></F>
                </QUD>
                <QUD string = "Was wird auf dem Bordcomputer zum Verbrauch angezeigt?">
                    <F><SEGMENT> Eine hohe Acht wird angezeigt; </SEGMENT></F>
                    <QUD string = "Wie ist der Kraftstoffverbrauch auf der Testfahrt bei schneller Fahrt?">
                        <QUD string = "Was machen sportiv gefahreren Strecken?">
                            <CON><SEGMENT> mit sportivem Ehrgeiz gefahrene Strecken </SEGMENT></CON>
                            <F><SEGMENT> sorgen für zweistelligen Durchfluss (10,2 Liter, mit steigender Tendenz) des Treibstoffs. </SEGMENT></F>
                        </QUD>
                    </QUD>
                </QUD>
            </QUD>
        </QUD>
        <QUD string = "Was ist mit der Ausstattung des SQ5?">
            <!-- DT: Ausstattung-->
            <QUD string = "Was ist mit der Preisliste (zur Ausstattung)?">
                <QUD string = "Wann sollte man hart im Nehmen sein?">
                    <CON><Segment> Hart im Nehmen sollte man auch </Segment></CON><!-- abhärten wie im Abschnitt davor: Analogie-Signal "auch" der Informatiionsstruktur geht auch bei RST nicht -->
                    <F><Segment> beim Studieren der Preisliste sein. </Segment></F>
                </QUD>
            </QUD>
            <QUD String = "Was ist der Grund dafür?">

                <QUD String = "Was ist mit den Grundkosten?">

                    <QUD String = "Was kostet der SQ5?">
                        <RES><Segment> Zwar </Segment></RES><!-- Kontrast zwischen Basispreis und Extrakosten durch Extras -->
                        <F><Segment> ist der ab 67.750 Euro erhältliche SQ5 </Segment></F>

                        <QUD String = "Was ist im Preis inklusive?">
                            <RES><Segment> unter anderem </Segment></RES>
                            <F><Segment id = "1.6.1.1.1.1.1.1"> mit 20-Zoll-Alus, </Segment></F>
                            <F><Segment id = "1.6.1.1.1.1.1.2"> einer markanten Frontschürze, </Segment></F>
                            <F><Segment id = "1.6.1.1.1.1.1.3"> Spiegelgehäuse in Aluminiumoptik, </Segment></F>
                            <F><Segment id = "1.6.1.1.1.1.1.4"> Leder-Sportsitzen </Segment></F>
                            <RES><Segment> und </Segment></RES>
                            <F>
                                <Segment id = "1.6.1.1.1.1.5"> einer elektrisch öffnenden </Segment>
                                <RES><Segment> und </Segment></RES>
                                <Segment id = "1.6.1.1.1.1.5"> schließenden Heckklappe ordentlich ausgestattet, </Segment>
                            </F>

                            <QUD String = "Welche Gelegenheit gibt es noch?">
                                <RES><Segment> doch </Segment></RES>
                                <CON>
                                    <Segment> gibt es noch </Segment>
                                    <NAI><Segment> reichlich </Segment></NAI><!-- lizensiert durch Zählen der Extras, die kosten (gleichzeitig feststehende Redewendung "reichlich Gelegenheit") -->
                                    <Segment> Gelegenheit </Segment>
                                </CON>
                                <F><Segment> zu weiteren Ausgaben.</Segment></F>
                            </QUD>
                        </QUD>
                    </QUD>
                </QUD>

                <QUD String = "Was ist mit den Extrakosten?">

